{
  "cli": {
    "version": ">= 13.2.5",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "image": "latest"
      },
      "android": {
        "image": "latest"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium",
        "image": "latest"
      },
      "android": {
        "image": "latest"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium",
        "image": "latest"
      },
      "android": {
        "image": "latest"
      },
      "env": {
        "EXPO_PUBLIC_PRIVACY_POLICY_URL": "https://opticalrxnow.com/privacy"
      }
    }
  },
  "submit": {
    "production": {}
  }
}