{
  "expo": {
    "name": "Optical Rx Now",
    "slug": "optical-rx-now",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "opticalrxnow",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "androidStatusBar": {
      "backgroundColor": "#0a1628",
      "barStyle": "light-content"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alamotte.opticalrxnow",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "Take photos of your eyeglass and contact lens prescriptions",
        "NSPhotoLibraryUsageDescription": "Select prescription photos from your gallery",
        "NSLocationWhenInUseUsageDescription": "Find nearby optical stores like Sam's Club",
        "NSHealthShareUsageDescription": "This app does not access Health data",
        "NSHealthUpdateUsageDescription": "This app does not access Health data"
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.alamotte.opticalrxnow",
      "versionCode": 1,
      "namespace": "com.alamotte.opticalrxnow",
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.alamotte.opticalrxnow",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0a1628"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-image.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0a1628"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Import prescription photos from your gallery",
          "cameraPermission": "Take photos of your prescriptions to store them"
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Find nearby optical stores for eyewear shopping"
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow Optical Rx Now to use Face ID for secure access to your prescriptions"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          },
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 24,
            "buildToolsVersion": "34.0.0",
            "kotlinVersion": "1.8.22",
            "newArchEnabled": false
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "3f66043f-24e3-41b5-b596-4cd63fd1e5e1"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "alamotte1956"
  }
}